{
    "name": "steevobb/owncloudallowed",
    "type": "phpbb-extension",
    "description": "An extension which allows you to add autorisation for owncloud connections",
    "homepage": "http://www.steevo.fr",
    "version": "1.0.0",
    "keywords": [
        "phpbb",
        "extension",
        "owncloud"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Philippe Ousset",
            "email": "steevo@steevo.fr",
            "homepage": "http://www.steevo.fr",
            "role": "Developper"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Owncloud Allowed",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.4-RC1,<3.2@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/owncloud_allowed",
            "filename": "version_check"
        }
    }
}